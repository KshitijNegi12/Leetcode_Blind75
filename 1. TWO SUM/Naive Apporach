class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int n=nums.size();
        // first index which iterate through all element except last
        for(int i=0;i<n-1;i++){
// then second index start from i+1 index and goes untill n is reached
            for(int j=i+1;j<n;j++){
                if(nums[i]+nums[j]==target){
                    return {i,j};
                }
            }
        }
        return {};
    }
};
